{% extends 'base.html.twig' %}

{% block title %}Trade{% endblock %}

{% block body %}
    <h1>Trade</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ trade.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ trade.name }}</td>
            </tr>
            <tr>
                <th>RegistrationNumber</th>
                <td>{{ trade.registrationNumber }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ trade.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('trade_index') }}">back to list</a>

    <a href="{{ path('trade_edit', {'id': trade.id}) }}">edit</a>

    {{ include('trade/_delete_form.html.twig') }}
{% endblock %}
